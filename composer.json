{
    "name": "cybex/transmorpher-client",
    "description": "A client package for Laravel applications that use the Transmorpher media server.",
    "keywords": [
        "cybex",
        "transmorpher",
        "transmorpher-client"
    ],
    "homepage": "https://github.com/cybex-gmbh/transmorpher-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marco Szulik",
            "email": "webdevelopment@cybex-online.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cybex\\Transmorpher\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cybex\\Transmorpher\\TransmorpherServiceProvider"
            ],
            "aliases": {
                "Transmorpher": "Cybex\\Transmorpher\\TransmorpherFacade"
            }
        }
    }
}

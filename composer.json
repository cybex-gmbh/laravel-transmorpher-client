{
    "name": "cybex/laravel-transmorpher-client",
    "description": "A client package for Laravel applications that use the Transmorpher media server.",
    "keywords": [
        "cybex",
        "laravel",
        "transmorpher",
        "transmorpher-client",
        "laravel-transmorpher-client"
    ],
    "homepage": "https://github.com/cybex-gmbh/laravel-transmorpher-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marco Szulik",
            "email": "webdevelopment@cybex-online.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-sodium": "*",
        "illuminate/support": "^9.0|^10.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Transmorpher\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Transmorpher\\TransmorpherServiceProvider"
            ]
        }
    }
}
